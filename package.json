{
  "name": "zcui-vue",
  "version": "1.0.0",
  "description": "Vuejs varient for zcui projects.",
  "config": {
    "src": "./src"
  },
  "scripts": {
    "test": "npm-run-all test:*",
    "dev": "cross-env NODE_ENV=development BABEL_ENV=development webpack-dev-server",
    "prod": "cross-env NODE_ENV=production BABEL_ENV=production webpack -p",
    "test:lint": "eslint $npm_package_config_src",
    "test:flow": "flow $npm_package_config_src",
    "test:jest": "jest $npm_package_config_src --coverage",
    "development": "npm run dev",
    "production": "npm run prod",
    "analyze:state": "webpack --profile --json > ./public/build/analyze.json",
    "analyze:server": "webpack-bundle-analyzer ./public/build/analyze.json -m server",
    "analyze": "npm-run-all analyze:*",
    "precommit": "npm-run-all test:flow test:lint",
    "prepush": "npm run test",
    "commitmsg": "validate-commit-msg"
  },
  "pre-commit": [
    "flow",
    "lint",
    "test"
  ],
  "keywords": [
    "zcui",
    "vuejs"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:hamidraza/zcui-vue.git"
  },
  "bugs": {
    "url": "https://github.com/hamidraza/zcui-vue/issues"
  },
  "homepage": "http://zcui.zoomcar.com",
  "author": "Hamid Raza <hamid.raza@zoomcar.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "dotenv": "^4.0.0",
    "eslint": "^3.18.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "flow-bin": "^0.42.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "node-sass": "^4.5.0",
    "npm-run-all": "^4.0.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.14.1",
    "validate-commit-msg": "^2.12.1",
    "vue": "^2.2.4",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.4",
    "vuex": "^2.2.1",
    "webpack": "^2.2.1",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-dev-server": "^2.4.2"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "zcui": {
    "variant": "vue"
  }
}
